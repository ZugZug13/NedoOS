;****************************************************************************
;** LEVEL 2 STUFF (DATA, SPRITES AND PROGRAMMING)                          **
;****************************************************************************

;                IF STARTLEV=2

                PHASE LEVELDATA

LEVEL2DATA
                DB $20,$30+FLOROF     ; HERO X,Y
                DB $00                ; MAPX
                DB $00                ; MAPY
                DB 0                  ; 0=SCROLL L/R - l=SCROLL U/D
                DB $CA,$6A            ; WEAPON ROOM X,Y INIT
                DW $48D8              ; DOOR XY
                DB $CA,$6A            ; RIGHTMAX,DOWNMAX
                DB $A0,$6A            ; SETUD,SETLR
                DW FLOOR2             ; FLOOR BLOCKS
                DW WALL2              ; WALL BLOCKS
                DB $25,$FF            ; LADDER BLOCKS - $25,$26 - $FF,$FF
                DB $85                ; CRAWL ON BLOCK
                DB $FF,$FF            ; SLIDE UP
                DB $FF,$FF            ; SLIDE DOWN
                DW LEV2BADX,LEV2BADX2 ; LEVEL BADDY INITS X,EXTRA X
                DW LEV2BADY           ; LEVEL BADDY INITS Y
                DW L2CHRS             ; CHRS
                DW L2ATRS             ; ATRS
                DW L2BLOX             ; BLOX
                DW L2MAPX             ; MAPX
                DW L2MAPY             ; MAPY
                DB 114                ; MAPXMAXSIZE

                CALL PRTARMYCHOC:CALL PRTCOGS:CALL L2PRTTANK:RET
                JP RESETLEV2BAD
                JP SMODCOGS
WAPTABLE2       DB $03,2,$01,2,$0A,2,$04,2,$06,3,$02,2
                JP KILLCHOC           ; KILL BIG
                JP INITTANK2          ; INIT BIG 1
                JP INITCONVEYOR       ; INIT BIG 2
                JP ARMYISSUE          ; INIT BIG 3
                JP INITCHOC           ; INIT BIG 4

;***************************************

RESETLEV2BAD    XOR A:LD (ISBIGON),A
                LD (PRTARMYCHOC+1),A
                LD (PRTCOGS+1),A:LD (PRTCHOC+1),A:LD (L2PRTTANK+1),A
                LD HL,L2TANKATRS1:LD (TANKATRS+1),HL
                LD HL,L2KILLTANK:LD (KILLBIG+1),HL
                LD HL,L2TANKTURRET1:LD (TUR1),HL
                LD HL,L2TANKTURRET2:LD (TUR2),HL
                LD HL,L2TANKTURRET3:LD (TUR3),HL

                LD HL,BAR2:LD DE,BCR1:LD BC,3*5:LDIR

                LD IX,COGS:LD B,24
RESPLAT         LD (IX+00),8:INC IX:DJNZ RESPLAT
                RET

BAR2            DW BADDYCRAWL21:DB 12,16,12   ; BAD CRAWL
                DW BADDYCRAWL22:DB 12,16,12   ; BAD CRAWL
                DW BADDYCRAWL23:DB 12,16,12   ; BAD CRAWL

;***************************************

FLOOR2          DB 3+2+3+2+8+2+2+4+1+4+1
                DB #84,#00,#01
                DB #02,#03
                DB #04,#07,#05
                DB #06,#2F
                DB #31,#08,#09,#0A,#29,#2C,#0B,#0C
                DB #5E,#85
                DB #32,#31
                DB #24,#22,#23,#21
                DB #20
                DB #0D,#0E,#0F,#10
                DB #2D

WALL2           DB 2+2+2+1+2+3+2+2+2
                DB #11,#4D
                DB #13,#12
                DB #14,#15
                DB #28
                DB #17,#16
                DB #1B,#18,#2E
                DB #19,#1A
                DB #55,#54
                DB #70,#19

;***************************************

LEV2BADX        DB $10,$01,$64,KEY             ; BADDY
                DB $18,$01,$64,KEY             ; BADDY
                DB $20,$00,$40,JUMP+KEY        ; BADDY
                DB $38,$FF,$38                 ; TANK
                DB $50,$01,$64,KEY             ; BADDY
                DB $58,$01,$64,KEY             ; BADDY
                DB $60,$00,$40,JUMP+KEY        ; BADDY
                DB $78,$02,$40,JUMP+KEY        ; BADDY
                DB $80,$03,$64,KEY             ; BADDY
                DB $88,$03,$64,KEY             ; BADDY
                DB $94,$04,$E0,$50             ; TANK TOP
                DB $96,$FB,$05                 ; SCROLL
                DB $FF,$FF                     ; END OF DATA
LEV2BADY        DB $0E,$FD                     ; CRAWL ARMY
                DB $16,$FD                     ; CRAWL ARMY
                DB $1E,$FD                     ; CRAWL ARMY
                DB $22,$FD                     ; CRAWL ARMY
                DB $28,$04,$B0,$80             ; TANK TOP
                DB $32,$05,$40,$30             ; TANK TOP
                DB $36,$FE,1                   ; PLATFORMS ON
                DB $55,$FE,0                   ; PLATFORMS OFF
                DB $56,$FC                     ; CHOCOLATE BAR
                DB $68,$04,$C0,$80             ; TANK TOP
                DB $FF,$FF                     ; END OF DATA
LEV2BADX2       DB $C0,$01,$64,KEY             ; BADDY
                DB $C6,$01,$64,KEY             ; BADDY
                DB $FF,$FF                     ; END OF DATA

;****************************************************************************
;** PRT CHOCOLATE BAR                                                      **
;****************************************************************************

SMODARMY        LD A,(PRTARMYCHOC+1):OR A:RET Z
                LD A,(ISSUEY+3):ADD A,H:LD (ISSUEY+3),A:OR A:RET P
                XOR A:LD (PRTARMYCHOC+1),A:RET

ARMYISSUE       LD A,$54:LD (ISSUEY+3),A:LD A,12:LD (ARMYCNT+1),A
                LD A,1:LD (PRTARMYCHOC+1),A:RET
PRTARMYCHOC     LD A,0:PUSH AF:CALL PRTCHOC:POP AF:OR A:RET Z
ARMYCNT         LD A,0:INC A:LD (ARMYCNT+1),A:AND $0F:RET NZ
                CALL FINDDEAD:RET NZ
ISSUEX          LD A,(ARMYCNT+1):AND $10:SUB $10:AND $E0:LD (IX+XPOS),A
ISSUEY          LD (IX+YPOS),$54:LD (IX+TEMP),0
                LD (IX+PHASE),0:LD (IX+COUNT),$04
                BIT 7,(IX+XPOS):JR Z,AFTER2
                LD (IX+PHASE),4:LD (IX+COUNT),$FC
AFTER2          LD (IX+TYPE),7:LD (IX+ACTION),8:RET

INITCHOC        LD A,1:LD (PRTCHOC+1),A:LD (NOSCROLL+1),A
                LD A,4:LD (CHOCD+1),A:LD HL,$58B0:LD (CHOCX+1),HL
                LD HL,KILLCHOC:LD (KILLBIG+1),HL
                LD A,$60:LD (BIGHITS),A
                LD A,3:JP PLAYMUSIC

KILLCHOC        XOR A:LD (NOSCROLL+1),A:LD (PRTCHOC+1),A
                LD (ISBIGON) ,A
                PUSH BC:PUSH IX:LD DE,(CHOCX+1)
                PUSH DE:LD A,E:ADD A,$20:LD E,A:CALL INITEXPNORND:POP DE
                PUSH DE:LD A,E:ADD A,$40:LD E,A:CALL INITEXPNORND:POP DE
                CALL INITEXPNORND:POP IX:POP BC:RET

PRTCHOC         LD A,0:OR A:RET Z:LD (ISBIGON),A
CHOCANI         LD A,1:XOR $40:LD (CHOCANI+1),A

CHOCX           LD DE,$58B0:LD A,E
CHOCD           ADD A,4:LD E,A
                CP 8-$20:JR Z,SWAPCHOC
                CP $F8-$40:JR NZ,NOSWAPCH
SWAPCHOC        LD A,(CHOCD+1):NEG :LD (CHOCD+1),A
NOSWAPCH        LD (CHOCX+1),DE

                PUSH DE:LD HL,CHOCATRS:LD BC,$0C04:CALL BAXATR:POP DE

                PUSH DE:LD A,E:ADD A,$20:LD E,A:LD HL,CHOCENG
                LD BC,$040C:CALL BAXIT:POP DE

                LD A,(POS):AND 2:JR NZ,SKPMID:PUSH DE
                LD HL,CHOCMID:LD BC,$0204:LD A,E:ADD A,$28:LD E,A
                LD A,D:ADD A,$10:LD D,A:CALL BAXIT:POP DE
SKPMID
                LD A,D:ADD A,8:LD D,A:PUSH DE
                LD A,E:ADD A,8:LD E,A
                LD HL,CHOCWING:LD BC,$0308:CALL BAXIT:POP DE
                PUSH DE:LD HL,CHOCWINGM:LD BC,$0108:CALL MASKCXB:POP DE
                PUSH DE:LD A,E:ADD A,$40:LD E,A
                LD HL,CHOCWING:LD BC,$0308:CALL BAXITMIR:POP DE
                PUSH DE:LD A,E:ADD A,$58:LD E,A
                LD HL,CHOCWINGM:LD BC,$0108:CALL MASKCXBMIR:POP DE

                LD A,D:ADD A,$10:LD D,A
                LD HL,TRACK1:LD A,(POS):AND 1:JR Z,GOZO:LD HL,TRACK2
GOZO            PUSH HL:PUSH DE:LD BC,$0404:CALL BAXIT:POP DE:POP HL
                LD A,E:ADD A,$40:LD E,A:LD BC,$0404:CALL BAXITMIR

                LD HL,(CHOCX+1):LD A,H:LD H,$60:LD (BIGX1),HL
                LD L,A:LD H,$28:LD (BIGY1),HL

CHOCFIRE        LD A,0:INC A:LD (CHOCFIRE+1),A:AND 7:RET NZ
                LD DE,(CHOCX+1):LD A,(CHOCFIRE+1):AND #38
                RET Z:CP #38:RET Z
                CP #20:JR C,FJF:ADD A,$20
FJF             ADD A,E:LD E,A:LD A,0:LD B,4:JP INITMIS

CHOCATRS
                DB #00,#00,#00,#00,#04,#04,#04,#04,#00,#00,#00,#00
                DB #00,#04,#04,#04,#04,#04,#04,#04,#04,#04,#04,#00
                DB #00,#04,#04,#04,#04,#06,#06,#04,#04,#04,#04,#00
                DB #06,#06,#06,#06,#00,#00,#00,#00,#06,#06,#06,#06
CHOCWING
                DB #03,#F8,#FE,#03,#0D,#74,#F5,#E9,#7A,#F4,#D2,#0B,#6B,#D2,#F4,#E8
                DB #A5,#D7,#D7,#A5,#E8,#10,#48,#A0,#3F,#8F,#E0,#10,#50,#40,#00,#00
                DB #00,#C0,#0F,#3F,#7F,#DF,#F0,#F0,#DF,#7F,#7F,#DF,#F0,#E4,#AF,#BF
CHOCWINGM
                DB #FF,#00,#FF,#00,#FF,#00,#FC,#00,#F8,#03,#F0,#06,#E0,#0E,#C0,#1D
                DB #80,#3C,#80,#38,#00,#70,#00,#60,#00,#42,#80,#45,#C0,#4A,#E0,#05
CHOCENG
                DB #3F,#A5,#FF,#FC,#02,#00,#FF,#40,#BF,#00,#7F,#F9,#F1,#3F,#1C,#9E
                DB #8E,#3E,#1F,#E1,#01,#00,#3E,#80,#84,#7F,#15,#41,#A1,#2A,#00,#82
                DB #45,#55,#55,#42,#04,#00,#00,#20,#1F,#FF,#FF,#F8,#04,#00,#00,#40
                DB #2A,#AA,#AA,#A8,#50,#55,#55,#15,#2A,#00,#00,#A9,#51,#FF,#FF,#90
                DB #41,#00,#00,#8A,#FD,#20,#08,#BF,#7E,#19,#30,#7E,#7D,#B7,#DA,#BE
                DB #7E,#9B,#B3,#7E,#7D,#B7,#DA,#BE,#5E,#19,#30,#7A,#FC,#20,#08,#3F
TRACK1
                DB #00,#00,#00,#00,#0C,#00,#00,#D0,#BD,#F7,#DF,#76,#F0,#BE,#EF,#9B
                DB #40,#00,#00,#02,#F6,#7D,#DF,#67,#37,#DF,#7D,#F4,#08,#82,#20,#08
TRACK2
                DB #00,#00,#00,#00,#1E,#00,#00,#60,#D7,#7D,#F7,#C3,#98,#EF,#FB,#CE
                DB #A0,#00,#00,#01,#7D,#DF,#F7,#3D,#1D,#F7,#DF,#7D,#82,#20,#08,#02
CHOCMID
                DB #00,#00,#80,#02,#06,#C0,#DF,#F6,#E6,#CF,#DF,#F6,#06,#C0,#80,#02

;***************************************
;** PRT COGS WITH PLATFORMS           **
;***************************************

SMODCOGS        CALL SMODARMY
                LD A,(PRTCOGS+1):OR A:RET Z
                LD IX,COG1:LD DE,6:LD B,6
SMODALLXBLP5    LD A,(IX+02):ADD A,H:LD (IX+02),A
                ADD IX,DE:DJNZ SMODALLXBLP5:RET

INITCONVEYOR    CALL GETDATA:LD (PRTCOGS+1),A:OR A:RET Z
                LD A,1:LD (PRTCOGS+1),A:PUSH IX:PUSH DE:PUSH BC
                LD IX,COG1:LD B,3:LD DE,6:LD A,$78
INITCONV        LD (IX+01),$18:LD (IX+02),A:ADD IX,DE
                LD (IX+01), $C8: LD (IX+02),A:ADD IX, DE
                ADD A,$28:DJNZ INITCONV:POP BC:POP DE:POP IX:RET

PRTCOGS         LD A,1:OR A:RET Z
COGNUM          LD A,0:CPL :LD (COGNUM+1),A
                LD HL,COGSP1:OR A:JR Z,FEMO:LD HL,COGSP2
FEMO

COG1            LD DE,$0818:CALL PRTCOG:LD DE,$08C8:CALL PRTCOG
                LD DE,$3018:CALL PRTCOG:LD DE,$30C8:CALL PRTCOG
                LD DE,$5818:CALL PRTCOG:LD DE,$58C8:CALL PRTCOG

                XOR A:LD (HAD1+1),A
                LD IX,COGS
                LD C,-8
                LD DE,(COG1+01):LD (PLAT1),DE:CALL PRTPLATCOGS
                LD A,(PLATX+1):XOR $18:LD (PLATX+1),A
                LD C,8
                LD DE,(COG1+13):LD (PLAT2),DE:CALL PRTPLATCOGS
                LD A,(PLATX+1):XOR $18:LD (PLATX+1),A
                LD C,-8
                LD DE,(COG1+25):LD (PLAT3),DE:CALL PRTPLATCOGS

                LD A,(POS):AND 3:CP 1:JR Z,HASBEEN:OR A:RET NZ
                LD A,(PLATX+1):SUB 8:AND $18:LD (PLATX+1),A
                CP $18:RET NZ

                LD HL,COGS+01:LD DE,COGS+00:LD BC,7
                LD A,(DE):LDIR :LD (DE),A
                LD HL,COGS+14:LD DE,COGS+15:LD BC,7
                LD A,(DE):LDDR :LD (DE),A
                LD HL,COGS+17:LD DE,COGS+16:LD BC,7
                LD A,(DE):LDIR :LD (DE),A:RET

HASBEEN         LD A,(HEROX+1)
HAD1            ADD A,0:CP $20:JR NC,OVER20:LD A,$20
OVER20          CP $C0:JR C,LESSC0:LD A,$C0
LESSC0          LD (HEROX+1),A:LD (GUNX+1),A:RET

PRTCOG          PUSH HL:PUSH DE:LD BC,$2020:CALL ISHEROINBOX:POP DE:POP HL
                LD B,4:PUSH HL
PRTCOGBLP       PUSH BC:PUSH DE:PUSH HL:LD A,D:OR A:CALL P,BAXITA
                POP HL:LD DE,2+32+4:ADD HL,DE
                POP DE:LD A,D:ADD A,8:LD D,A
                POP BC:DJNZ PRTCOGBLP:POP HL:RET

PRTPLATCOGS     LD A,D:ADD A,$20:LD D,A
PLATX           LD E,$18:LD B,8
PLATLOOP        LD A,(IX+00):OR A:JP Z,NEXTPLAT

                PUSH BC:PUSH DE:CALL PRTPLATCOG:POP DE
                LD A,D:OR A:JP M,SKPALLTHIS
                PUSH DE
                PUSH IX:DEC IX:DEC IX:LD BC,$1020:CALL ISBULINBOX:POP IX
                POP DE
SKPCOLI
                PUSH DE :LD A,(HEROX+2):LD L,A:LD H,$0F
                LD A,D:SUB 4:LD E,A:LD D,$07
                CALL HITA:JR C,NOITSNOT:POP DE
                PUSH DE:LD A,(HEROX+1):ADD A,8:LD L,A:LD H,$0F
                LD D,$1F:CALL HITA:JR C,NOITSNOT:POP DE

                LD A,D:SUB $10:LD (SHIM+1),A
                LD A,1:LD (STH+1),A
                XOR A:LD (HEROF+1),A:LD (HEROG+1),A

                POP BC:LD A,C:LD (HAD1+1),A

                LD A,(GUNDIR+1):CP $FF:CALL Z,FEST

                LD A,(HEROP+1):AND 7:OR 16:LD (HEROP+1),A
                LD A,(GUNDIR+1):AND 7:LD (GUNDIR+1),A
                LD A,(FUDLR):AND $0F:JR NZ,NZX
                LD A,(HEROP+1):OR 24:LD (HEROP+1),A
NZX             LD A,(HEROCRAWL+1):OR A:JR Z,NEXTPLAT
                LD A,(GUNDIR+1):AND 7:OR 56:LD (GUNDIR+1),A
                LD A,(HEROP+1):AND 7:OR 40:LD (HEROP+1),A:JR NEXTPLAT

NOITSNOT        POP DE
SKPALLTHIS      POP BC
NEXTPLAT        INC IX
                LD A,E:ADD A,$20:LD E,A:DEC B:JP NZ,PLATLOOP:RET

PRTPLATCOG      LD A,D:OR A:RET M
                LD HL,PLATFORMSPR:JP BAXITA

COGS            DS 24,0
PLAT1           DW 0
PLAT2           DW 0
PLAT3           DW 0

COGSP1          DB 4,1*8
                DB #00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#C0,#1B,#00
                DB #00,#1B,#C0,#00,#80,#C3,#1B,#02,#05,#93,#C7,#40,#E0,#E2,#C7,#0B
                DB #46,#46,#46,#46
                DB 4,1*8
                DB #03,#FF,#FD,#E0,#C0,#FF,#FF,#19,#0E,#F8,#3F,#80,#00,#CF,#E3,#06
                DB #00,#CC,#37,#70,#80,#0B,#90,#01,#6F,#92,#4B,#F8,#F8,#85,#21,#6F
                DB #46,#56,#56,#46
                DB 4,1*8
                DB #6F,#21,#85,#F8,#F8,#4B,#92,#6F,#01,#90,#0B,#80,#00,#37,#CC,#00
                DB #02,#E3,#CF,#00,#80,#3F,#F8,#06,#0D,#FF,#FF,#C0,#E0,#F9,#FF,#1B
                DB #46,#56,#56,#46
                DB 4,1*8
                DB #07,#C7,#E6,#E0,#40,#C3,#B3,#0B,#05,#1B,#C1,#80,#00,#C0,#1B,#00
                DB #00,#1B,#C0,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00
                DB #46,#46,#46,#46

COGSP2          DB 4,1*8
                DB #00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#60,#00,#00,#68,#58,#00
                DB #00,#B8,#CE,#00,#00,#DE,#7C,#00,#01,#BC,#9E,#00,#E0,#3C,#DE,#00
                DB #46,#46,#46,#46
                DB 4,1*8
                DB #18,#5F,#F9,#C0,#30,#FD,#3F,#37,#37,#F0,#1E,#F0,#F8,#E7,#C7,#6F
                DB #0F,#98,#1B,#F8,#E0,#8B,#90,#37,#1B,#20,#05,#80,#80,#85,#2D,#0D
                DB #46,#56,#56,#46
                DB 4,1*8
                DB #05,#21,#B5,#80,#C0,#05,#20,#1D,#33,#91,#0B,#E0,#F8,#1B,#98,#0F
                DB #6F,#C7,#E7,#F8,#F0,#1E,#F0,#37,#37,#3F,#FD,#30,#C0,#F8,#1F,#14
                DB #46,#56,#56,#46
                DB 4,1*8
                DB #00,#DE,#3C,#00,#00,#DC,#BE,#01,#01,#7C,#DE,#00,#00,#5E,#B8,#00
                DB #00,#08,#68,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00,#00
                DB #46,#46,#46,#46

PLATFORMSPR
                DB 4,1*8
                DB #3F,#FF,#FF,#00,#C0,#FF,#FF,#4F,#73,#FF,#FF,#F0,#FC,#FF,#FF,#7C
                DB #3F,#00,#FF,#00,#FF,#FF,#7F,#0F,#03,#7F,#FF,#FF,#FF,#FF,#7F,#00
                DB #42,#42,#42,#42

;*****************************************************************************
;** PRT TANK                                                                **
;*****************************************************************************

L2KILLTANK      XOR A: LD (NOSCROLL+1),A:LD (L2PRTTANK+1),A
                LD (ISBIGON),A:PUSH BC:PUSH IX
                LD DE,(L2TANKX+1):LD A,D:ADD A,$10:LD D,A
                PUSH DE:LD A,E:ADD A,$10:LD E,A:CALL INITEXPNORND:POP DE
                PUSH DE:LD A,D:ADD A,$20:LD D,A:CALL INITEXPNORND:POP DE
                LD A,E:ADD A,$20:LD E,A:CALL INITEXPNORND
                POP IX:POP BC:RET

L2CHTD          LD A,(L2TANKD+1):NEG :LD (L2TANKD+1),A:JR L2GTNK

INITTANK2       LD A,1:LD (L2PRTTANK+1),A:LD A,-4:LD (L2TANKD+1),A
                LD A,$20:LD (BIGHITS),A:LD A,13:LD (L2TNM+1),A
                CALL GETDATA:LD D,A:LD E,$D8:LD (L2TANKX+1),DE:RET

L2PRTTANK       LD A,0:OR A:RET Z
                LD (ISBIGON),A:LD (NOSCROLL+1),A

L2TANKX         LD DE,$FFFF:LD A,E
L2TANKD         ADD A,0:CP $D8:JP NC,L2CHTD:CP $78:JP C,L2CHTD
                LD E,A:LD (L2TANKX+1),DE
L2GTNK
                LD A,(POS):AND $13:JR NZ,L2TANKZ

                LD DE,(L2TANKX+1):LD A,E:ADD A,$18:LD E,A
                PUSH DE:LD A,D:ADD A,$18:LD D,A
                LD A,(HEROX+2):ADD A,$10:CP D:LD L,13:JR C,L2FHU
                SUB $10:CP D:LD L,12:JR C,L2FHU:LD L,11
L2FHU           LD A,L:LD (L2TNM+1),A:POP DE:LD A,D:ADD A,$08:LD D,A
                LD A,L:LD B,4:CALL INITBAL

L2TANKZ         LD DE,(L2TANKX+1)
                LD A,3*4:LD (PRTSPRBLP-1),A
                PUSH DE:LD A,E:ADD A,$10:LD E,A:PUSH DE
L2TNM           LD A,0:SUB 12:NEG :INC A:LD DE,192
                LD HL,L2TANKTURRET1:CALL MULT:POP DE:CALL NC,PRTSPR:POP DE

                PUSH DE:LD A,E:ADD A,$10:LD E,A:LD HL,L2TANKATRS1
                LD BC,$0403:CALL BAXATR:POP DE
                LD A,D:ADD A,$18:LD D,A
                PUSH DE:LD HL,L2TANKATRS2
                LD BC,$0904:CALL BAXATR:POP DE

                PUSH DE:LD A,E:ADD A,$18:LD E,A:LD HL,L2TANKMID2
                LD BC,$0308:CALL NC,BAXIT:POP DE

                PUSH DE:LD HL,L2TANKMID1
                LD BC,$0308:CALL MASKCXB:POP DE

                PUSH DE:LD A,E:ADD A,$30:LD E,A:LD HL,L2TANKMID1
                LD BC,$0308:CALL NC,MASKCXBMIR:POP DE

                LD A,D:ADD A,$0F:LD D,A
                PUSH DE :CALL L2PRTWHEEL: POP DE
                LD A,E:ADD A,$18:LD E,A:JR C,L2FKJ
                PUSH DE:CALL L2PRTWHEEL:POP DE
                LD A,E:ADD A,$18:LD E,A:JR C,L2FKJ
L2PRTWHEEL      LD A,(POS):AND 2:LD HL,L2TANKWHEEL1:JR Z,LZWEEL
                LD HL,L2TANKWHEEL2
LZWEEL          PUSH DE:LD BC,$0304:CALL BAXIT:EX DE,HL:POP DE
                LD A,D:ADD A,8:LD D,A:LD BC,$0308:CALL MASKCXB
L2FKJ           LD A,(L2TANKX+1):ADD 16:LD L,A:LD H,31:LD (BIGX1),HL
                LD HL,(L2TANKX+2):LD H,$37:LD (BIGY1),HL:RET

L2TANKMID1
                DB #FF,#00,#FF,#00,#00,#01,#00,#7E,#FC,#00,#FF,#00,#FF,#00,#E0,#03
                DB #00,#80,#00,#60,#00,#1C,#FF,#00,#FC,#00,#00,#E3,#00,#80,#00,#80
                DB #00,#1F,#F8,#03,#F0,#04,#00,#7C,#00,#60,#00,#F6,#00,#63,#E0,#09
                DB #E0,#0B,#00,#1F,#00,#86,#00,#4E,#00,#FC,#C0,#12,#C0,#16,#00,#E3
                DB #00,#BC,#00,#6A,#00,#1F,#80,#25,#80,#2C,#00,#F5,#00,#7C,#00,#6A
                DB #00,#AB,#00,#43,#00,#7D,#00,#55,#00,#74,#00,#00,#00,#00,#00,#40
L2TANKMID2
                DB #C4,#00,#46,#01,#00,#00,#1F,#FF,#FE,#00,#00,#00,#1F,#FF,#FE,#56
                DB #6B,#3D,#7F,#FF,#FE,#46,#20,#55,#80,#64,#CC,#C6,#60,#00,#27,#66

                DB #E0,#04,#00,#94,#00,#45,#0F,#40,#07,#20,#00,#4A,#00,#54,#C0,#0C
                DB #80,#14,#00,#38,#00,#3C,#07,#20,#0F,#40,#00,#00,#00,#00,#80,#12

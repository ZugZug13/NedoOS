INITARMY        LD A,1:LD (ARMY+1),A:LD (NOSCROLL+1),A
                LD A,255:LD (ARMYCOUNT+1),A
                LD HL,ARMYDATA:LD (ARMYPOINTER+1),HL
                RET

ARMY            LD A,0:OR A:RET Z
ARMYCOUNT       LD A,0:INC A:LD (ARMYCOUNT+1),A
ARMYPOINTER     LD HL,ARMYDATA:CP (HL):RET NZ:INC HL
                LD A,(HL):INC HL:CP FIN:JR Z,KILLARMY
                LD DE,(BADDATA+1):PUSH DE:LD (BADDATA+1),HL
                CALL GOINITBADA:LD HL,(BADDATA+1):LD (ARMYPOINTER+1),HL
                POP HL:LD (BADDATA+1),HL:RET

KILLARMY        XOR A:LD (ARMY+1),A:LD (NOSCROLL+1),A
                LD A,8+1+2:LD (FLOOR6),A
                LD A,$FF:LD (CHANGEFLOOR1),A
                RET

ARMYDATA        DB $08,$01,$64,KEY      ; BADDY
                DB $10,$03,$64,0        ; BADDY
                DB $18,$01,$64,0        ; BADDY
                DB $1C,$0A,$00,$64,$04  ; CRAWLER
                DB $20,$03,$64,KEY      ; BADDY
                DB $28,$01,$64,0        ; BADDY
                DB $2C,$0A,$E8,$64,$FC  ; CRAWLER
                DB $30,$03,$64,0        ; BADDY
                DB $38,$01,$64,KEY      ; BADDY
                DB $3C,$0A,$E8,$64,$FC  ; CRAWLER
                DB $40,$03,$64,0        ; BADDY
                DB $44,$0A,$00,$64,$04  ; CRAWLER
                DB $48,$01,$64,0        ; BADDY
                DB $70,FIN

;***************************************

INITTV          LD A,8+1+2:LD (FLOOR6),A
                LD A,2:CALL PLAYMUSIC

WALKTOPLACE
                LD A,(HEROX+2):CP $64:JR Z,SMX:ADD A,4:AND $FC
                LD (HEROX+2),A:SUB $10:LD (GUNX+2),A:JR SKPSMX
SMX             LD A,(HEROX+1):LD E,A:CP $30:JR Z,SWITCHTVON
                LD A,0:RLA :ADD A,A:DEC A
                ADD A,A:ADD A,A:ADD A,A
                ADD A,E:LD (HEROX+1),A:LD (GUNX+1),A
                LD A,E:CP $30:LD A,0:RLA :ADD A,A:ADD A,A:LD C,A
                LD A,(HEROP+1):DEC A:AND 3:OR C:XOR 20:LD (HEROP+1),A
                LD A,6:SUB C:LD (GUNDIR+1),A
SKPSMX          CALL DOALLBACKBITS
                CALL BEEPER
                CALL BEEPER
                JR WALKTOPLACE

;FIN             EQU $FF
TVSTATIC        EQU $FE
TVSCROLL        EQU $FD
FACEOPEN        EQU $01
HELPER          EQU $02
GRANDPA         EQU $03
FACECLOSED      EQU $04

KILLTV          LD A,0:LD (PRTTV+1),A:LD (NOSCROLL+1),A
                LD A,$47:LD (ATTR),A
                RET

SWITCHTVON      LD A,1:LD (GUNDIR+1),A:LD A,24:LD (HEROP+1),a
                CALL SPRINT:DB PRSIZE,0,PREOF

                LD HL,TVSEQ
TVLOOP          LD A,(HL):INC HL:CP FIN:JR Z,KILLTV
                CP TVSTATIC:JR Z,STATICTV:CP TVSCROLL:JP Z,SCROLLTV
                LD (PRTTV+1),A:PUSH HL:CALL DOALLBACKBITS:POP HL
                LD B,(HL):INC HL:PUSH HL:CALL BFLY:POP HL
                LD A,$46:LD (ATTR),A
                LD E,(HL):INC HL:LD D,(HL):INC HL
                LD C,(HL):INC HL:LD B,(HL):INC HL
                PUSH HL:CALL PRTBOX:POP HL:CALL PRINT
                LD A,$47:LD (ATTR),A:LD B,(HL):INC HL
                PUSH HL:CALL BFLY:POP HL:JR TVLOOP

STATICTV        LD B,(HL):INC HL:LD A,$FF:LD (PRTTV+1),A
STATICTVBLP     PUSH HL:PUSH BC:CALL DOALLBACKBITS:POP BC:POP HL
                DJNZ STATICTVBLP:JR TVLOOP

PRTTV           LD A,0:OR A:RET Z:CP $FF:JR Z,STATIC
                DEC A:LD HL,TVFACES:LD DE,2+$F0+$1E:CALL MULT
                LD DE,$2870:JP BAXITA

STATIC          LD A,(POS):AND 3:INC A:LD C,A
                LD HL,BACKSCR+$050E:LD DE,32-5:LD B,$28
                LD A,(POS):AND 4:RRA :RRA :DEC A
STATBLP         LD (HL),A:INC L:LD (HL),A:INC L:LD (HL),A:INC L
                LD (HL),A:INC L:LD (HL),A:INC L:LD (HL),A:ADD HL,DE
                DEC C:JP NZ,COMPZ
                CPL :LD C,4
COMPZ           DJNZ STATBLP

                LD HL,BACKATR+$00AE:LD DE,32-6:LD C,5
CZMPZ           LD B,6
CAMPZ           LD (HL),$47:INC L:DJNZ CAMPZ:ADD HL,DE
                DEC C:JR NZ,CZMPZ:RET

PRTBOX          PUSH DE:PUSH BC:LD A,B:SUB 2:LD B,A

                PUSH BC:INC E:LD A,C:SUB 2:LD (BOXSIZEOK1+5),A
BOXSIZEOK1      PUSH DE:CALL SPRINT:DB PRMULTY,10," ",PREOF
                POP DE:DEC E:INC D:POP BC

                LD A,C:LD (BOXSIZEOK2+6),A
BOXSIZEOK2      PUSH BC:PUSH DE:CALL SPRINT:DB PRMULTY,10," ",PREOF
                POP DE:POP BC:INC D:DJNZ BOXSIZEOK2

                INC E:LD A,C:SUB 2:LD (BOXSIZEOK3+4),A
BOXSIZEOK3      CALL SPRINT:DB PRMULTY,10," ",PREOF

                POP BC:POP DE:LD HL,BOXUDGS:CALL MASKCHR
                LD A,E:ADD A,C:DEC A:LD E,A:CALL MASKCHR
                LD A,D:ADD A,B:DEC A:LD D,A:CALL MASKCHR
                LD A,E:SUB C:INC A:LD E,A:CALL MASKCHR
                INC D:LD A,C:SRL A:ADD A,E:LD E,A
MASKCHR         PUSH DE:CALL CHRAD:PUSH BC:LD B,8
MASKCHRBLP      LD A,(DE):AND (HL):INC HL:OR (HL):INC HL
                LD (DE),A:INC D:DJNZ MASKCHRBLP
                LD A,D:RRA :RRA :RRA :DEC A:AND 3:OR 88:LD D,A
                LD A,$46:LD (DE),A:POP BC:POP DE:RET

TVSEQ           DB TVSTATIC,65
                DB FACEOPEN,25
                DB 04,00,14,05
                DB PRAT,04+3,01,"HA-HA-HA"
                DB PRAT,04+3,02,"WE  HAVE"
                DB PRAT,04+1,03,"YOUR FAMILY.",PREOF,250
                DB TVSCROLL
                DB HELPER,25
                DB 06,00,10,03
                DB PRAT,06+2,01,"HELP!!",PREOF,250
                DB GRANDPA,25
                DB 03,00,17,05
                DB PRAT,03+1,01,"MY RESEARCH HAS"
                DB PRAT,03+2,02,"PUT MY FAMILY"
                DB PRAT,03+5,03,"AT RISK",PREOF,250
                DB FACECLOSED,25
                DB 03,00,21,05
                DB PRAT,03+3,01,"KEEP COMING!!"
                DB PRAT,03+1,02,"YOU AND YOUR FAMILY"
                DB PRAT,03+5,03,"WILL DIE!!!",PREOF,250
                DB TVSTATIC,65
                DB FIN

SCROLLTV        XOR A:LD (PRTTV+1),A:PUSH HL:CALL DOALLBACKBITS
                LD IX,SCRTVMAP:LD C,25:LD A,$46:LD (ATTR),A
SCROLLTVCLP     PUSH BC:LD DE,SCROLLLINE:LD A,(IX+00):CALL DUMPTVCHR
                LD A,(IX+01):CALL DUMPTVCHR:LD A,(IX+02):CALL DUMPTVCHR
                LD A,(IX+03):CALL DUMPTVCHR:LD A,(IX+04):CALL DUMPTVCHR
                POP BC:PUSH BC:LD B,8
SCROLLTVBLP     PUSH BC
                LD B,3:CALL BFLY:CALL SCROLLTELE

                DB #06,#00,#10,#FE,#10,#FE,#10,#FE,#10,#FE

                CALL BACKTOREAL
                POP BC:PUSH BC:LD A,C
                CP 23:JR NC,NODAD:CP 19:JR C,NODAD
                LD DE,$0603:LD BC,$0306:CALL PRTBOX
                CALL SPRINT:DB PRAT,04,07,"DAD!",PREOF
                JR NOGAD
NODAD           CP 11:JR NC,NOGOD:CP 07:JR C,NOGOD
                LD DE,$0503:LD BC,$0407:CALL PRTBOX
                CALL SPRINT:DB PRAT,04,06,"OH MY",PRAT,04,07,"GOD!!",PREOF
                JR NOGAD
NOGOD           CALL BEEPER
NOGAD
                POP BC:DJNZ SCROLLTVBLP:LD DE,5:ADD IX,DE
                POP BC:DEC C:JP NZ,SCROLLTVCLP:LD B,6*8
SCROLLTVOFF     PUSH BC:LD B,5:CALL BFLY:CALL SCROLLTELE:POP BC
                DJNZ SCROLLTVOFF:POP HL
                LD A,$47:LD (ATTR),A:JP TVLOOP

SCROLLTELE      LD C,40:LD HL,$40B3
                EXX:LD HL,SCROLLLINE:EXX
SCROLLTELECLP   EXX:SLA (HL):INC HL:EXX:PUSH HL:LD B,6
SCROLLTELEBLP   RL (HL):DEC HL:DJNZ SCROLLTELEBLP
                POP HL:CALL PIXDN
                DEC C:JR NZ,SCROLLTELECLP:RET

BACKTOREAL      LD HL,BACKSCR+$500+3:LD DE,$40A3:LD B,5*8
BACKTOREALBLP   PUSH HL:PUSH DE:PUSH BC:LD BC,7:LDIR
                POP BC:POP DE:POP HL:EX DE,HL:CALL PIXDN:EX DE,HL
                LD A,L:ADD A,32:LD L,A:ADC A,H:SUB L:LD H,A
                DJNZ BACKTOREALBLP:RET

DUMPTVCHR       LD L,A:LD H,0:ADD HL,HL:ADD HL,HL:ADD HL,HL
                LD BC,SCRTVCHR:ADD HL,BC:LD C,H
                LDI:LDI:LDI:LDI:LDI:LDI:LDI:LDI:RET

SCROLLLINE      EQU BACKSCR


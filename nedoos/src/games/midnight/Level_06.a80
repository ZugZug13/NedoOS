;***************************************************************************
;** LEVEL 6 STUFF (DATA, SPRITES AND PROGRAMMING)                         **
;***************************************************************************

;                IF STARTLEV=6
                PHASE LEVELDATA

LEVEL6DATA      DB $20,$20+FLOROF     ; HERO X,Y
                DB $00                ; MAPX
                DB $00                ; MAPY
                DB 0                  ; 0=SCROLL L/R - 1=SCROLL U/D
                DB $5C,$50            ; WEAPON ROOM X,Y INIT
                DW $38D8              ; DOOR XY
                DB $5C,$50            ; RIGHTMAX,DOWNMAX
                DB $24,$50            ; SETUD,SETLR
                DW FLOOR6             ; FLOOR BLOCKS
                DW WALL6              ; WALL BLOCKS
                DB $FF,$FF            ; LADDER BL0CK5 - $FF,$FF - $FF,$FF
                DB $FF                ; CRAWL ON BLOCK
                DB $3C,$FF            ; SLIDE UP
                DB $FF,$FF            ; SLIDE DOWN
                DW LEV6BADX,LEV6BADX2 ; LEVEL BADDY INITS X,EXTRA X
                DW LEV6BADY           ; LEVEL BADDY INITS Y
                DW L6CHRS             ; CHRS
                DW L6ATRS             ; ATRS
                DW L6BLOX             ; BLOX
                DW L6MAPX             ; MAPX
                DW L6MAPY             ; MAPY
                DB 72                 ; MAPXMAXSIZE

                CALL NOCOGS:CALL ARMY:CALL PRTTV:RET
                JP RESETLEV6BAD
                JP RETURN
WAPTABLE6       DB $01,3,$09,3,$04,3,$07,4,$06,4,$05,2
                JP RETURN             ; KILL BIG
                JP INITLEV6COGS       ; INIT BIG 1
                JP INITARMY           ; INIT BIG 2
                JP INITTV             ; INIT BIG 3
                JP RETURN             ; INIT BIG 4

;***************************************

RESETLEV6BAD    XOR A:LD (ISBIGON),A:LD (ARMY+1),A:LD (NOCOGS+1),A
                LD (PRTTV+1),A
                LD A,8+1+2:LD (FLOOR6),A
                LD HL,L6TANKATRS1:LD (TANKATRS+1),HL
                LD HL,L6TANKTURRET1:LD (TUR1),HL
                LD HL,L6TANKTURRET2:LD (TUR2),HL
                LD HL,L6TANKTURRET3:LD (TUR3),HL

                LD HL,BAR6:LD DE,BCR1:LD BC,3*5:LDIR

                RET

BAR6            DW BADDYCRAWL61:DB 12,16,12    ; BAD CRAWL
                DW BADDYCRAWL62:DB 12,16,12    ; BAD CRAWL
                DW BADDYCRAWL63:DB 12,16,12    ; BAD CRAWL

;***************************************

FLOOR6          DB 4+4+1+2
                DB #0A,#0B,#0C,#0E
                DB #04,#05,#02,#3C
CHANGEFLOOR1    DB #27
                DB #00,#01

WALL6           DB 1
                DB #FF

;***************************************

LEV6BADX        DB $02,$01,$24,KEY     ; BADDY
                DB $04,$05,$D8,$60     ; TANK TOP
                DB $08,$01,$24,KEY     ; BADDY
                DB $20,$FB,$02         ; SCROLL
                DB $FF,$FF             ; END OF DATA
LEV6BADY        DB $02,$FF,$01         ; ONE COG
                DB $10,$FE             ; ARMY
                DB $22,$FF,$02         ; ONE COG
                DB $30,$FE             ; ARMY
                DB $42,$FF,$03         ; ONE COG
                DB $50,$FD             ; T.V. SEQUENCE
                DB $FF,$FF             ; END OF DATA
LEV6BADX2       DB $40,$01,$64,KEY     ; BADDY
                DB $48,$01,$64,KEY     ; BADDY
                DB $50,$01,$64,KEY     ; BADDY
                DB $58,$05,$D8,$60     ; TANK TOP
                DB $FF,$FF             ; END OF DATA


CAGLEN          EQU 5
CAG1            DS 3*CAGLEN
CAG1INIT        DB $E0,-8,$20,$00,$00
                DB $E0,-8,$20,$08,$03
                DB $E0,-8,$20,$10,$06

INITLEV6COGS    CALL GETDATA:LD (NOCOGS+1),A:LD (NOSCROLL+1),A
                LD HL,CAG1INIT:LD DE,CAG1:LD BC,3*CAGLEN:LDIR
                CP 3:RET Z:LD C,A:XOR A
                LD (CAG1+2+CAGLEN*2),A
                LD A,C:CP 2:RET Z:XOR A:LD (CAG1+2+CAGLEN),A:RET

NOCOGS          LD A,0:OR A:RET Z:LD IX,CAG1:LD B,3
CAGBLP          PUSH BC:CALL PRTCAG:POP BC:LD DE,CAGLEN:ADD IX,DE
                DJNZ CAGBLP:RET

PRTCAG          LD A,(IX+03):OR A:JR Z,CAGON:DEC (IX+03):RET
CAGON           LD A,(IX+02):OR A:RET Z:LD D,(IX+01):LD A,(IX+00)
                ADD A,D:LD E,A:CP $00:JR Z,SWD:CP $E8:JR NZ,NSWD
SWD             LD A, D: NEG :LD D,A
NSWD            LD (IX+00),E:LD (IX+01),D:INC (IX+04)

                LD D,$20:PUSH DE:LD A,(IX+04):AND 1:LD HL,COG6SPR
                JR NZ,NOLM:LD HL,COG6SPR2
NOLM            LD BC,$0410:CALL MASKCXB:POP DE

                PUSH DE:LD A,(IX+04):AND 7:ADD A,A
                LD HL,SCOGOFS:CALL ADDHLA
                LD A,(HL):ADD A,E:LD E,A:INC HL
                LD A,(HL):ADD A,$20:LD D,A
                LD A,(POS):LD HL,SCOG1:AND 1:JR Z,NOML:LD HL,SCOG2
NOML            LD BC, $0208:PUSH DE:CALL MASKCXB:POP DE

                LD BC,$1818
                PUSH IX:LD IX,BADCOG-2:CALL ISHEROINBOX:POP IX:POP DE

                PUSH DE:LD A,E:ADD A,8:LD E,A
                LD D,$28:LD HL,COG6ATR:LD BC,$0202:CALL BAXATR:POP DE

                PUSH DE:LD D,$28:LD BC,$2020:CALL ISBULORHERO
                POP DE:LD A,(IX+02):OR A:RET NZ

                PUSH IX:LD D,$20:CALL INITEXPNORND:POP IX
                LD A,(NOCOGS+1):DEC A:LD (NOCOGS+1),A:RET NZ
KILLCOG6        LD (NOSCROLL+1),A
                LD A,8+1:LD (FLOOR6),A
                LD A,$27:LD (CHANGEFLOOR1),A:RET

BADCOG          DB 0

SCOGOFS         DW $F808,$00F8
                DW $08F8,$1800
                DW $1808,$1018
                DW $0818,$F810

COG6ATR         DB #44,#44
                DB #44,#44


;********************************•*****•*************************************
;** LEVEL 7 STUFF (DATA, SPRITES AND PROGRAMMING)                          **
;****************************************************************************

;                IF STARTLEV=7

                PHASE LEVELDATA

LEVEL7DATA      DB $20,$40+FLOROF     ; HERO X,Y
                DB $00                ; MAPX
                DB $00                ; MAPY
                DB 0                  ; 0=SCROLL L/R - l-=SCROLL U/D
                DB $C6,$00            ; WEAPON ROOM X,Y INIT
                DW $28D8              ; DOOR XY
                DB $C6,$00            ; RIGHTMAX,DOWNMAX
                DB $FF,$FF            ; SETUD,SETLR
                DW FLOOR7             ; FLOOR BLOCKS
                DW WALL7              ; WALL BLOCKS
                DB $FF,$FF            ; LADDER BLOCKS - $FF,$FF - $FF,$FF
                DB $FF                ; CRAWL ON BLOCK
                DB $14,$9C            ; SLIDE UP
                DB $04,$05            ; SLIDE DOWN
                DW LEV7BADX,LEV7BADX2 ; LEVEL BADDY INITS X,EXTRA X
                DW LEV7BADY           ; LEVEL BADDY INITS Y
                DW L7CHRS             ; CHRS
                DW L7ATRS             ; ATRS
                DW L7BLOX             ; BLOX
                DW L7MAPX             ; MAPX
                DW L7MAPY             ; MAPY
                DB 128                ; MAPXMAXSIZE

                CALL PRTSHIP:CALL PRTTUR:LD HL,0:RET
                JP RESETLEV7BAD
                JP SMODWAPS
WAPTABLE7       DB $09,4,$04,4,$04,4,$08,4,$0A,6,$00,2
                JP RETURN             ; KILL BIG
                JP INITSHIP           ; INIT BIG 1
                JP INITTUR            ; INIT BIG 2
                JP RETURN             ; INIT BIG 3
                JP RETURN             ; INIT BIG 4

;*****************•*********************

SMODWAPS        LD A,(MAPY):CP $F0:RET Z

                LD IX,TURS:LD DE,2:LD B,3
SMTUR           LD A,(IX+00):CP $E8:JR NC,HANS:ADD A,L:LD (IX+00),A
HANS            ADD IX,DE:DJNZ SMTUR

                LD IX,WAPXYS:LD DE,5:LD B,6
SMODWAPSBLP     LD A,(IX+02):CP $FF:JR Z,NOSMODWAPS
                LD A,(IX+00):ADD A,L:LD (IX+00),A
                CP $08:JR C,KILWAP:CP $E8:JR C,NOSMODWAPS
KILWAP          LD (IX+02),$FF
NOSMODWAPS      ADD IX,DE:DJNZ SMODWAPSBLP:RET

RESETLEV7BAD    XOR A:LD (ISBIGON),A:LD (PRTSHIP+1),A:LD (PRTTUR+1),A
                LD IX,TURS:LD DE,2:LD B,3
RESTUR          LD (IX+00),$FF:ADD IX,DE:DJNZ RESTUR
                RET

;***************************************

FLOOR7          DB 2+2+2+4
                DB #01,#02
                DB #04,#05
                DB #74,#06
                DB #14,#9C,#9A,#99

WALL7           DB 3
                DB #00,#0C,#0A

;***************************************

LEV7BADX        DB $08,$01,$64,KEY     ; BADDY
                DB $10,$01,$64,KEY     ; BADDY
                DB $18,$01,$64,KEY     ; BADDY
                DB $20,$FE,$00,$D8,$10 ; SHIP GUN
                DB $28,$FE,$02,$D8,$08 ; SHIP GUN
                DB $30,$FB,$04         ; SCROLL
                DB $32,$FE,$04,$D8,$00 ; SHIP GUN
                DB $38,$FF             ; SHIP
                DB $88,$01,$64,KEY     ; BADDY
                DB $90,$01,$64,KEY     ; BADDY
                DB $98,$01,$64,KEY     ; BADDY
                DB $A0,$01,$64,KEY     ; BADDY
                DB $A8,$01,$64,KEY     ; BADDY
                DB $B0,$01,$64,KEY     ; BADDY
                DB $B8,$01,$64,KEY     ; BADDY
                DB $FF,$FF             ; END OF DATA
LEV7BADY        DB $FF,$FF             ; END OF DATA
LEV7BADX2       DB $FF,$FF             ; END OF DATA

;****************************************************************************
;** MOVE SHIP SPRITE                                                       **
;****************************************************************************

INITTUR         CALL GETDATA:LD E,A:LD D,0:LD HL,TURS:ADD HL,DE
                EX DE,HL:CALL GETDATA:LD (DE),A:INC DE
                CALL GETDATA:LD (DE),A:LD A,1:LD (PRTTUR+1),A:RET

PRTTUR          LD A,0:OR A:RET Z
                LD IX,TURS:LD B,3
PRTTURBLP       LD A,(IX+00):CP $E8:JR NC,NATUR:PUSH BC
                LD E,A:LD D,(IX+01):LD HL,SHIPGUN23:CALL BAXITA:POP BC
NATUR           LD DE,2:ADD IX,DE:DJNZ PRTTURBLP:RET

TURS            DS 6,0

;*********************

INITSHIP        XOR A:LD (PRTTUR+1),A
                INC A:LD (PRTSHIP+1),A:LD (SHIPDIR+1),A
                LD (SWAPPED+1),A
                LD HL,SHIPXYS+2:LD DE,3:LD B,10+6
INITSHIPBLP     LD (HL),8:ADD HL,DE:DJNZ INITSHIPBLP:RET

KILLSHIP        LD A,$90:LD (PRTSHIP+1),A:XOR A:LD (SHIPY+1),A
                LD A,$74:LD (MAPX),A:RET

SINKSHIP        INC A:LD (PRTSHIP+1),A:LD (NOSCROLL+1),A:AND 7
                JR NZ,FARTY
                LD HL,BACKSCR2+$A8F:LD DE,BACKSCR2+$B5F:LD BC,$A90:LDDR
                LD HL,BACKSCR2:LD DE,BACKSCR2+1:LD BC,$CF:LD (HL),$00
                LDIR
                LD HL,BACKATR2+$151:LD DE,BACKATR2+$16B:LD BC,$152:LDDR
                LD HL,BACKATR2:LD DE,BACKATR2+1:LD BC,$19:LD (HL),$47
                LDIR
                LD A,(SHIPY+1):ADD A,8:LD (SHIPY+1),A
FARTY           CALL RAND:AND $1F
SHIPY           ADD A,$00:LD D,A
                LD A,(RND2):LD E,A
                JP INITEXP

PRTSHIP         LD A,0:OR A:RET Z:LD (NOSCROLL+1),A:JP M,SINKSHIP

                LD A,(POS):AND 3:LD HL,WAVESPR:LD DE,$40:CALL MULT
                LD A,(MAPX):ADD A,2:AND 3:ADD A,A:ADD A,A:ADD A,A
                LD E,A:LD D,$60:LD B,8
                LD A,(MAPX):CP $6E:JR C,NA7:DEC B
                CP $70:JR C,ANA:DEC B
                CP $74:JR C,ANA:DEC B
                JR ANA
NA7             LD A,(MAPX):CP $38:JR NC,NA8
                DEC B:LD E,$20
NA8
ANA
WAVEBLP         PUSH HL:PUSH DE:PUSH BC:LD BC,$0408
                CALL BAXIT:POP BC:POP DE:POP HL
                LD A,E:ADD A,$20:LD E,A:DJNZ WAVEBLP

CANT            LD A,0:INC A:LD (CANT+1),A

                XOR A:LD (NOOFEM+1),A
                LD IX,SHIPXYS:LD B,16
                LD A,(MAPX):SUB 3:LD C,A

PRTSHPBLP
                PUSH BC:LD A,(IX+02):OR A:JP Z,NXTSHP
                LD A, (NOOFEM+1):INC A:LD (NOOFEM+1),A

                LD A,(IX+00):SUB C:CP 32-3:JP NC,NXTSHP
                ADD A,A:ADD A,A:ADD A,A:LD E,A:LD D,(IX+01)

                LD A,D:CP $40:JR C,NOTBIGGUN
                AND $3F:LD D,A:PUSH DE
                LD A,(CANT+1):AND 15:CP 3:JR C,JEL:LD A,2
JEL             LD HL,SHIPGUN21:LD DE,2+160+20:CALL MULT:POP DE

                PUSH DE:CALL BAXITA:POP DE
                PUSH DE:LD BC,$1F1F:CALL ISBULINBOX:POP DE
                LD A,(CANT+1):AND 15:JR NZ,NXTSHP
                LD A,E:ADD A,8:LD E,A:LD A,D:ADD A,$10:LD D,A
                LD B,4:XOR A:PUSH IX:CALL INITMIS:POP IX:JR NXTSHP

NOTBIGGUN       LD A,(HEROX+1):SUB E:LD HL,SHIPSPR2:LD BC,$0808
                CP 32:JR C,OK_Y_ALL:CP -32:JR NC,OK_Y_ALL
                LD A,(HEROX+1):SUB E:LD HL,SHIPSPR1:LD BC,$000A
                JP C,OK_Y_ALL:LD HL,SHIPSPR3:LD BC,$1006
OK_Y_ALL        PUSH BC:PUSH DE:CALL BAXITA:POP DE:POP BC

                PUSH DE:CALL RAND:LD A,(RND1):AND 63:JR NZ,OKAT
                LD A,E:ADD A,B:LD E,A:LD B,4:LD A,C
                PUSH IX:CALL INITBIL:POP IX
OKAT            POP DE:LD BC,$0F1F:CALL ISBULINBOX

NXTSHP          POP BC:LD DE,3:ADD IX,DE:DEC B:JP NZ,PRTSHPBLP
NOOFEM          LD A,0:OR A:JP Z,KILLSHIP

JCNTY           LD A,0:XOR 1:LD (JCNTY+1),A:RET NZ
SWAPPED         LD A,0:OR A:LD A,0:LD (SWAPPED+1),A:JR NZ,NOSWAPSHIP
                LD A,(MAPX):CP $1B*2:JR Z,SWAPSHIPDIR
                CP $3A*2:JR NZ,NOSWAPSHIP
SWAPSHIPDIR     LD A,1:LD (SWAPPED+1),A
                LD A,(SHIPDIR+1):NEG :LD (SHIPDIR+1),A:RET
NOSWAPSHIP      XOR A:LD (SMODHERO+1),A:CALL SHIPDIR
                LD A,1:LD (SMODHERO+1),A:RET
SHIPDIR         LD A,1:OR A:JP P,DOSCRIGHT:JP DOSCLEFT

